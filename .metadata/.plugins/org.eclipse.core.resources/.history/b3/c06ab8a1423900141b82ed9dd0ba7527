 * 
 */

/**
 * @author maddierook
 *
 */
public class Accuracy_Precision_Calculator {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		// define accuracy variables
		double measuredValue;
		double exactValue;
		double percentError;
		
		//calculate percent error for density of water
		measuredValue = 1.06;
		exactValue = 1.00;
		percentError = ((Math.abs(measuredValue - exactValue) / exactValue) * 100);
		
		//output percent error
		System.out.println("Accuracy, via Percent Error:");
		System.out.println(percentError);
		System.out.println();
		
		
		//define precision variables
		double averageValue;
		double x1;
		double x2;
		double x3;
		double x4;
		double x5;
		double x6; 
		double x7;
		double x8;
		double x9;
		double sumOfSquares;
		double standardDeviation;
		double standardError;
		int numberOfTrials;
				
		//calculate sum of squares
		x1 = 1.30;
		x2 = 1.20;
		x3 = 1.10;
		x4 = 0.98;
		x5 = 1.01;
		x6 = 0.95;
		x7 = 1.05;
		x8 = 0.99;
		x9 = 0.99;
		averageValue = 1.06;
		sumOfSquares = (Math.abs(x1 - averageValue)*Math.abs(x1 - averageValue) + Math.abs(x2 - averageValue)*Math.abs(x2 - averageValue) + Math.abs(x3 - averageValue)*Math.abs(x3 - averageValue) + Math.abs(x4 - averageValue)*Math.abs(x4 - averageValue) + Math.abs(x5 - averageValue)*Math.abs(x5 - averageValue) + Math.abs(x6 - averageValue)*Math.abs(x6 - averageValue) + Math.abs(x7 - averageValue)*Math.abs(x7 - averageValue) + Math.abs(x8 - averageValue)*Math.abs(x8 - averageValue) + Math.abs(x9 - averageValue)*Math.abs(x9 - averageValue));

		//calculate standard error of the mean for density of water
		numberOfTrials = 9;
		standardDeviation = (Math.sqrt((sumOfSquares*sumOfSquares) / (numberOfTrials - 1) ));
		standardError = (standardDeviation / Math.sqrt(numberOfTrials));
				
		//output standard error of the mean
		System.out.println("Precision, via Standard Error of the Mean:");
		System.out.println(standardError);
	}

}
